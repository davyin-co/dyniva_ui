{# note: header.html.twig must be include in header block, the sub theme be able to override header #}
{% block header %}
  {% include '@dyniva_ui/ui/pages/header.html.twig' %}
{% endblock %}

<div id="wall" class="page-title promo">
  <div class="inner">
    <h1 class="page-title">面板组件</h1>
    <h5 class="title">面板组件带有明显的轮廓、常用来放置带标题和文字的内容块，使用户更好的聚焦内容。</h5>
  </div>
</div>
<div class="panel panel-ui">
  <section class="dy-components" id="panel">
    <div class="inner dy-components-inner">
      <div class="row">
        <div class="col-xs-12 col-md-2">
          <h2 class="section-header inner">面板</h2>
        </div>
        <div class="col-xs-12 col-md-10">
          <div class="section-item title-box">
            <h4 class="title inner">卡片风格</h4>
            <p class="rules inner">有明显的背景和轮廓。</p>
            <div class="dy-components-demo outside-tb">
              <div class="row">
                <div class="col-sm-4">
                  <div class="panel panel-card">
                    <div class="panel-row-larger t-c">
                      <img src="holder.js/128x128?bg=fff"
                           class="img-n radius-circle outside-b" width="256" height="256" alt="avatar">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《计算机与科学技术》
                      <h4>
                        <strong>张三</strong>
                      </h4>
                    </div>
                  </div>
                </div>
                <div class="col-sm-4">
                  <div class="panel panel-card">
                    <div class="panel-row-larger t-c">
                      <img src="holder.js/128x128?bg=fff"
                           class="img-n radius-circle outside-b" width="256" height="256" alt="avatar">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《基础物理》
                      <h4>
                        <strong>李四</strong>
                      </h4>
                    </div>
                  </div>
                </div>
                <div class="col-sm-4">
                  <div class="panel panel-card">
                    <div class="panel-row-larger t-c">
                      <img src="holder.js/128x128?bg=fff"
                           class="img-n radius-circle outside-b" width="256" height="256" alt="avatar">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《摄影基础》
                      <h4>
                        <strong>王五</strong>
                      </h4>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="dy-components-markup inner">
              <a class="btn btn-primary expand-code collapsed" role="button" data-toggle="collapse"
                 aria-expanded="false"
                 aria-controls="collapseCode">
                <i class="icon">&#xe818;</i> 开发说明
              </a>
              <div class="collapse">
                <div>
                  {% set blocks = '
<div class="panel panel-card">
  <div class="panel-row-larger t-c">
    <img src="holder.js/128x128?bg=fff"
          class="img-n radius-circle outside-b" width="256" height="256" alt="avatar">
    <p class="text-light outside-b">讲师 / 硕士生导师
      <br>
      主要课程：《计算机与科学技术》
    <h4>
      <strong>张三</strong>
    </h4>
  </div>
</div>' %}
                  <pre class="prettyprint lang-html">
                    {{ blocks }}
                    </pre>
                </div>
              </div>
            </div>
          </div>{# end section #}
          <div class="outside-tb-lg">

          </div>
          <div class="section-item title-box">
            <h4 class="title inner">对话风格</h4>
            <p class="rules inner">图片在下面。</p>
            <div class="dy-components-demo inner">
              <div class="row">
                <div class="col-sm-4 outside-b">
                  <div class="panel panel-talk">
                    <div class="t-c talk talk-bottom">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《计算机与科学技术》
                        <h4 class="outside-min-b">张三</h4>
                        <a href="#" class="text-color-primary">了解更多</a>
                      </p>
                    </div>
                    <div class="panel-talk-mark t-c">
                      <img src="holder.js/128x128?bg=eee"
                           class="img-n radius-circle panel-talk-media" width="256" height="256" alt="avatar">
                    </div>
                  </div>
                </div>
                <div class="col-sm-4 outside-b">
                  <div class="panel panel-talk">
                    <div class="t-c talk talk-bottom">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《物理基础》
                        <h4 class="outside-min-b">李四</h4>
                        <a href="#" class="text-color-primary">了解更多</a>
                      </p>
                    </div>
                    <div class="panel-talk-mark t-c">
                      <img src="holder.js/128x128?bg=eee"
                           class="img-n radius-circle panel-talk-media" width="256" height="256" alt="avatar">
                    </div>
                  </div>
                </div>
                <div class="col-sm-4 outside-b">
                  <div class="panel panel-talk">
                    <div class="t-c talk talk-bottom">
                      <p class="text-light outside-b">讲师 / 硕士生导师
                        <br>
                        主要课程：《摄影基础》
                        <h4 class="outside-min-b">王五</h4>
                        <a href="#" class="text-color-primary">了解更多</a>
                      </p>
                    </div>
                    <div class="panel-talk-mark t-c">
                      <img src="holder.js/128x128?bg=eee"
                           class="img-n radius-circle panel-talk-media" width="256" height="256" alt="avatar">
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="dy-components-markup inner">
              <a class="btn btn-primary expand-code collapsed" role="button" data-toggle="collapse"
                 aria-expanded="false"
                 aria-controls="collapseCode">
                <i class="icon">&#xe818;</i> 开发说明
              </a>
              <div class="collapse">
                <div>
                  {% set blocks = '
<div class="panel panel-talk">
  <div class="t-c talk talk-bottom">
    <p class="text-light outside-b">讲师 / 硕士生导师
      <br>
      主要课程：《计算机与科学技术》
      <h4 class="outside-min-b">张三</h4>
      <a href="#" class="text-color-primary">了解更多</a>
    </p>
  </div>
  <div class="panel-talk-mark t-c">
    <img src="holder.js/128x128?bg=eee"
          class="img-n radius-circle panel-talk-media" width="256" height="256" alt="avatar">
  </div>
</div>' %}
                  <pre class="prettyprint lang-html">
                      {{ blocks }}
                      </pre>
                </div>
              </div>
            </div>
          </div>{# end section #}
        </div>
      </div>
    </div>
  </section>
</div>

{# note: ui-footer.html.twig must be include in footer block, the sub theme be able to override footer #}
