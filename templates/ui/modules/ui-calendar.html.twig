      </div>
      <div class="dy-components-markup inner">
        <a class="btn btn-primary expand-code collapsed" role="button" data-toggle="collapse"
           aria-expanded="false" aria-controls="collapseCode">
          <i class="icon">&#xe818;</i> 开发说明
        </a>
        <div class="collapse">
          <div>
            <pre class="prettyprint lang-html">
            {%- filter escape -%}
              {% include'@dyniva_ui/ui/modules/calendar/dy-calendar-base.html.twig' with{
                options: '{"min": "2019-10-01", "max": "2020-01-01"}',
                placeholder: 'yyyy-MM-dd',
                label: '限定可选日期'
              } %}

              {% include'@dyniva_ui/ui/modules/calendar/dy-calendar-base.html.twig' with{
                options: '{"min": -7, "max": 7}',
                placeholder: 'yyyy-MM-dd',
                label: '前后若干天可选'
              } %}

              {% include'@dyniva_ui/ui/modules/calendar/dy-calendar-base.html.twig' with{
