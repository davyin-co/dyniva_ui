{{ attach_library('dyniva_ui/common.codeprettify') }}
{{ attach_library('dyniva_ui/plugin.holder') }}
<div id="ui-header" class="ui-preview-header navbar-one bg-color-primary sticky-header">
  <div class="sticky-wrapper">
    <div class="inner">
      <div class="f-l nav-branding outside-min-t inside-min-tb clearfix">
        <div class="f-r">
          <h2 id="site-name" class="nav-name">
            <a href="{{ base_path }}ui-preview/pages/index.html.twig" title="Home" class="text-white capitalize" rel="home">Dyniva Ui</a>
          </h2>
          <p id="site-slogan" class="nav-slogan text-white size-m">Version 2.1.0</p>
        </div>
      </div>
      <div class="f-r js-main-menu">
        {% block menu %}
          {% include '@dyniva_ui/ui/preview/ui-preview-menu.html.twig' %}
        {% endblock %}
        <button id="header-btns" class="hamburger hamburger--collapse" type="button">
          <span class="hamburger-box">
            <span class="hamburger-inner"></span>
          </span>
        </button>
      </div>
    </div>
  </div>
</div>
